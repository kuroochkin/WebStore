@model IEnumerable<WebStore.Models.Employee>

<!DOCTYPE html>

<html>

<head>
    <title>Сотрудники</title>
</head>

<body>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Фамилия</th>
                <th>Имя</th>
                <th>Отчество</th>
                <th>Возраст</th>
            </tr>
        </thead>

        <tbode>
            @foreach(var employee in Model)
            {
                <tr>
                    <td>@employee.Id</td>
                    <td>@employee.LastName</td>
                    <td>@employee.FirstName</td>
                    <td>@employee.Patronymic</td>
                    <td>@employee.Age</td>
                </tr>
            }
        </tbode>
    </table>
</body>

</html>

